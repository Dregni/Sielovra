#tabbarwidget QToolButton {
	border-radius: 12px;
	border: 2px solid transparent;
	height: 20px;
	width: 20px;
}

#tabbarwidget * {
	background: transparent;
}

#tabbar {
	color: scolor(text);
}

QSplitter {
	border-radius: 10px;
}

#bookmarks-bar {
	background: scolor(main, normal, 50);
	border: 5px solid black;
}

#bookmarks-bar QToolButton {
	background: scolor(accent, normal, 100);
}

QTabBar {
}

QTabBar::tab
{
	background: scolor(second, light, 100);
	border-radius: 20px;
	height: 40px;
	/* color: scolor(text, dark); */
}

QTabBar::tab:selected
{
	background: scolor(main, light, 200);
}

QTabBar::tab:!selected:hover
{
	background: scolor(second, light, 150) repeat-x;
}

#highlighted-new-tabsspace
{
	background: scolor(main, light, 200);
}

